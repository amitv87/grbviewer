    "@fortawesome/fontawesome-free-regular": "^5.0.10",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@fortawesome/react-fontawesome": "0.0.18",
    "@types/jszip": "^3.1.3",
    "@types/pako": "^1.0.0",
    "@types/react": "^16.3.12",
    "@types/react-data-grid": "^2.0.10",
    "@types/react-dom": "^16.0.5",
    "@types/react-table": "^6.7.9",
    "jszip": "^3.1.5",
    "pako": "^1.0.6",
    "react": "^16.3.2",
