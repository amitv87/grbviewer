{
  "name": "grbviewr",
  "version": "0.0.1",
  "description": "Gerber viewer for the web",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --mode development --inline --content-base .",
    "dist": "date -u +\"export const Build='%g%m%d.%H%M';\" >common/build.ts && webpack --mode production && npx uglify-es -c -m <dist/AsyncGerberParser.worker.js -o dist/AsyncGerberParser.worker.min.js && npx uglify-es -c -m <dist/bundle.js -o dist/bundle.js.tmp && sed 's/\\.worker\\.js/.worker.min.js/' <dist/bundle.js.tmp >dist/bundle.min.js && rm dist/bundle.js.tmp"
  },
  "keywords": [
    "gerber"
  ],
  "author": "Venelin Efremov <veffremov@iqury.us>",
  "repository": "https://github.com/ghent360/grbviewer.git",
  "license": "MIT",
  "dependencies": {
